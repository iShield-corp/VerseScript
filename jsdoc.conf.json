{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": false
    },
    "opts": {
      "destination": "./docs",
      "recurse": true
    }
  }
  